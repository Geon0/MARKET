		<property name="hosts">
			<list>
				<value>ip-172-31-27-197.ap-northeast-2.compute.internal</value>
			</list>
		</property>
		<property name="manage"><value>false</value></property>		
		<property name="sign"><value>302C021451C00C3F5AC8347DBFAE592AB3EC7D8E9DABD9F102145ACE2FCBDB365377730D6E724D83B3476FF69ADE</value></property>		
