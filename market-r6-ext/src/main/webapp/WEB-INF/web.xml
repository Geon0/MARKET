	<filter>
		<filter-name>LoginFilter</filter-name>
		<filter-class>com.kcube.sys.login.LoginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsl/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/mjsl/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>com.kcube.sys.jar.JarCheckListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.kcube.sys.deploy.DeployListener</listener-class>
	</listener>
	<listener>
	    <listener-class>com.kcube.sys.login.LoginSessionCollector</listener-class>
	</listener>
	<servlet>
		<servlet-name>AppServlet</servlet-name>
		<servlet-class>com.kcube.sys.AppServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>JslServlet</servlet-name>
		<servlet-class>com.kcube.lib.http.JslServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>JslServlet</servlet-name>
		<url-pattern>/jsl/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JslServlet</servlet-name>
		<url-pattern>/mjsl/*</url-pattern>
	</servlet-mapping>
		
	<servlet>
        <servlet-name>PortletContainerDesktopServlet</servlet-name>
        <servlet-class>com.kcube.ptl.jsrportlet.PortletDesktopServlet</servlet-class>
        <load-on-startup>3</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>PortletContainerDesktopServlet</servlet-name>
        <url-pattern>/jsr_desktop/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PortletContainerDesktopServlet</servlet-name>
        <url-pattern>/jsr_adm_desktop/*</url-pattern>
    </servlet-mapping>
    <filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsr_desktop/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/jsr_adm_desktop/*</url-pattern>
	</filter-mapping>
	
	<!-- AXIS2 WebService START -->
	<servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>AxisServlet</servlet-name>
        <servlet-class>org.apache.axis2.transport.http.AxisServlet</servlet-class>
        <load-on-startup>4</load-on-startup>
    </servlet>
	<servlet-mapping>
        <servlet-name>AxisServlet</servlet-name>
        <url-pattern>/ws/*</url-pattern>
    </servlet-mapping>
    <!-- AXIS2 WebService END -->
	
	<!-- WebDAV START -->
	<servlet>
        <servlet-name>WebDav servlet</servlet-name>
        <servlet-class>com.kcube.sys.webdav.WebDavServlet</servlet-class>
        <load-on-startup>5</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebDav servlet</servlet-name>
        <url-pattern>/webdav/*</url-pattern>
    </servlet-mapping>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/webdav/*</url-pattern>
	</filter-mapping>
	<!-- WebDAV END -->
	
